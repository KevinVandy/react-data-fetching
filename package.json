{
  "name": "@react-data-fetching/react-data-fetching",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev1-1": "turbo dev --filter 1-1-vite-componentdidmount",
    "dev1-2": "turbo dev --filter 1-2-vite-useeffect",
    "dev1-3": "turbo dev --filter 1-3-vite-reactquery",
    "dev1-4": "turbo dev --filter 1-4-vite-reactquery-refactor",
    "dev1-5": "turbo dev --filter 1-5-astro-react-spa",
    "dev2-1": "turbo dev --filter 2-1-nextjs-ssg",
    "dev2-2": "turbo dev --filter 2-2-astro-ssg",
    "dev2-3": "turbo dev --filter 2-3-astro-react-ssg",
    "dev3-1": "turbo dev --filter 3-1-nextjs-ssr",
    "dev3-2": "turbo dev --filter 3-2-remix-ssr",
    "dev3-3": "turbo dev --filter 3-3-sveltekit-ssr",
    "dev3-4": "turbo dev --filter 3-4-astro-ssr",
    "dev4-1": "turbo dev --filter 4-1-nextjs-rsc",
    "dev4-2": "turbo dev --filter 4-2-nextjs-rsc-react-query",
    "dev4-3": "turbo dev --filter 4-3-nextjs-rsc-react-query-streaming",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "serve": "turbo serve --filter 0-json-server"
  },
  "devDependencies": {
    "prettier": "^3.5.1",
    "turbo": "2.4.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.3.0"
}
