    "dev1-6": "turbo dev --filter 1-6-vite-tanstack-router",
    "dev1-7": "turbo dev --filter 1-7-vite-tanstack-router-loaders",
    "dev1-8": "turbo dev --filter 1-8-vite-tanstack-router-file-based-routing",
    "dev1-9": "turbo dev --filter 1-9-astro-react-spa",
    "dev2-1": "turbo dev --filter 2-1-nextjs-ssg",
